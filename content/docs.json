{
  "$schema": "https://mintlify.com/docs.json",
  "name": "cs2.sh",
  "theme": "linden",
  "colors": {
    "primary": "#B45309",
    "light": "#F97316",
    "dark": "#D97706"
  },
  "background": {
    "color": {
      "light": "#FAF9F6",
      "dark": "#161616"
    }
  },
  "fonts": {
    "heading": { "family": "JetBrains Mono", "weight": 700 },
    "body": { "family": "JetBrains Mono", "weight": 400 }
  },
  "icons": {
    "library": "lucide"
  },
  "logo": {
    "text": "cs2.sh",
    "href": "https://cs2.sh"
  },
  "navbar": {
    "links": [
      { "label": "Dashboard", "href": "https://cs2.sh/dashboard" }
    ],
    "primary": {
      "type": "button",
      "label": "Sign Up",
      "href": "https://cs2.sh/signup"
    }
  },
  "footer": {
    "socials": {
      "discord": "https://discord.gg/7DeT4rVnT9",
      "github": "https://github.com/idomanteu/cs2.sh-price-api"
    }
  },
  "metadata": {
    "og:site_name": "cs2.sh Docs",
    "og:url": "https://cs2.sh/docs"
  },
  "contextual": {
    "options": ["copy", "view", "claude", "chatgpt"]
  },
  "seo": {
    "indexHiddenPages": false
  },
  "navigation": {
    "groups": [
      {
        "group": "Getting Started",
        "icon": "terminal",
        "pages": ["introduction", "discord", "llms-txt"]
      },
      {
        "group": "Guides",
        "icon": "book-open",
        "pages": ["authentication", "rate-limits", "pricing"]
      },
      {
        "group": "Data",
        "icon": "database",
        "pages": ["endpoints", "field-availability", "variant-items"]
      },
      {
        "group": "API Reference",
        "icon": "code",
        "pages": [
          "api-reference/health",
          "api-reference/prices-latest",
          "api-reference/prices-latest-post",
          "api-reference/prices-history",
          "api-reference/liquidity",
          "api-reference/liquidity-post",
          "api-reference/archive-history",
          "api-reference/archive-csfloat"
        ]
      }
    ]
  }
}
